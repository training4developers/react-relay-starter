{
  "name": "react-relay-starter",
  "version": "1.0.0",
  "description": "Starter project for React/Relay/GraphQL",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "postinstall": "npm run build",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run server && npm run webpack-once && npm run update-schema && npm run webpack-once",
    "webpack": "webpack --config ./config/webpack.dev.js --watch",
    "webpack-once": "webpack --config ./config/webpack.dev.js",
    "server": "babel ./src --presets es2015,stage-0 --out-dir ./dist --source-maps --ignore www",
    "update-schema": "babel-node --presets es2015,stage-0 ./scripts/updateSchema.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/training4developers/react-relay-starter.git"
  },
  "author": "Eric W. Greene",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/training4developers/react-relay-starter/issues"
  },
  "homepage": "https://github.com/training4developers/react-relay-starter#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-relay-plugin": "^0.9.1",
    "bootstrap-loader": "^1.0.10",
    "css-loader": "^0.23.1",
    "eslint": "^3.0.1",
    "eslint-plugin-react": "^5.2.2",
    "exports-loader": "^0.6.3",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "imports-loader": "^0.6.5",
    "node-sass": "^3.8.0",
    "null-loader": "^0.1.1",
    "postcss": "^5.0.21",
    "postcss-loader": "^0.9.1",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.3",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-merge": "^0.14.0"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.2",
    "es6-promise": "^3.2.1",
    "express": "^4.14.0",
    "express-graphql": "^0.5.3",
    "graphql": "^0.6.1",
    "graphql-relay": "^0.4.2",
    "mongoose": "^4.5.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-relay": "^0.9.1",
    "whatwg-fetch": "^1.0.0"
  },
  "webServer": {
    "protocol": "http",
    "host": "localhost",
    "port": 3000,
    "folder": "dist/www"
  },
  "mongoServer": {
    "host": "localhost",
    "post": 27017,
    "dbName": "bootcamp"
  }
}
